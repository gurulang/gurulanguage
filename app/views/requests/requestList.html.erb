<center> <H3> LIST OF REQUESTS </H3> </center>

<table width="75%" align="center" border="1">
  <tr>
    <th>tutor</th>
    <th>Day</th>
    <th>Start</th>
    <th>End</th>
    <th>Start Time</th>
    <th>End Time</th>
    <th>title</th>
    <th>status</th>
	<th> </th>
	<th> </th>
  </tr>
 
<% @requests.each do |request| %>
  <tr>
    <td><%= request.tutorID %></td>
    <td><%= request.day %></td>
    <td><%= request.startDate %></td>
    <td><%= request.endDate %></td>
    <td><%= request.startTime %></td>
    <td><%= request.endTime %></td>
    <td><%= request.title %></td>
    <td><%= request.status %></td>
    <td><%= link_to 'Show', request %></td>
    <td><%= link_to 'Delete', request, :confirm => 'Are you sure?',
                                     :method => :delete %> </td>
  </tr>
<% end %>
</table>

